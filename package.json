    "prettier": "^3.4.2",
    "socket.io-mock": "^1.3.2",
    "testcontainers": "^10.16.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.7.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
