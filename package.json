    "@mui/material-nextjs": "^6.3.0",
    "@mui/x-charts": "^7.23.2",
    "@mui/x-date-pickers": "^7.23.3",
    "ajv-formats": "^3.0.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "knex": "^3.1.0",
    "next": "^15.1.2",
