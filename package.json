{
  "name": "class-interactor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "docker compose -f docker-compose.dev.yml up -d",
    "postdev": "docker compose -f docker-compose.dev.yml down",
    "dev": "trap 'exit 0' SIGINT; nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "lint": "next lint",
    "prepare": "husky install",
    "knex": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' npx knex"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.5",
    "@next/font": "13.1.1",
    "body-parser": "^1.20.1",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "knex": "^2.4.0",
    "next": "13.1.1",
    "pg": "^8.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.5.2",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/uuid": "^9.0.0",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.8",
    "testcontainers": "^9.9.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.9.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md,json}": "prettier --write"
  }
}
